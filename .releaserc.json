{
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "main",
    {
      "name": "dev",
      "prerelease": true
    }
  ],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "releaseRules": [
          {
            "type": "chore",
            "release": "patch"
          }
        ]
      }
    ],
    [
      "@semantic-release/release-notes-generator",
      {
        "preset": "conventionalcommits",
        "presetConfig": {
          "types": [
            {
              "type": "breaking",
              "section": "Features"
            },
            {
              "type": "chore",
              "section": "Bug fixes",
              "hidden": false
            }
          ]
        }
      }
    ],
    [
      "@semantic-release/github",
      {
        "successComment": false,
        "failComment": true,
        "labels": false,
        "releasedLabels": false
      }
    ]
  ]
}